// tip: each public class is put in its own file
public class main
{
    // tip: arguments are passed via the field below this editor
    public static void main(String[] args)
    {
       
        boolean var_boolean=true;
        int idade= 20;
        float peso= 90.7f;
        double altura= 1.90;
        char letra= 'c';
        String name= "João das Neves";
        System.out.println ("o conteúdo da variável boolean é :" +var_boolean);
        System.out.println ("o conteúdo da variável boolean é :" +idade);
        System.out.println ("o conteúdo da variável boolean é :" +peso);
        System.out.println ("o conteúdo da variável boolean é :" +altura);
        System.out.println ("o conteúdo da variável boolean é :" +letra);
        System.out.println ("o conteúdo da variável boolean é :" +name);

    }
}
